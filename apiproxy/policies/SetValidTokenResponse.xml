        <Payload contentType="application/json" variablePrefix="%" variableSuffix="#">{&quot;expires_in&quot;:&quot;%accesstoken.expires_in#&quot;, &quot;user&quot;:&quot;%accesstoken.username#&quot;}</Payload>
